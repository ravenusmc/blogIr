/* Bootstrap Gem parts below */

 @import "bootstrap-sprockets";
 @import "bootstrap";
 @import "font-awesome";

/* Over all CSS Styling */

body {
 	background-color: #0B4C5F;
 }

.container {
	background-color: rgba(255,255,255,0.8);
	margin-top: 25px;
	margin-bottom: 25px;
	padding-bottom: 10px;
	border-radius: 10px;
}

 .invisible {
 	display: none;
 }

 .blog_index {
 	background: rgba(255,255,255,0.8);
 }


/* Index Page CSS */

.jumbotron {
	text-align: center;
	margin-bottom: 0px;
	height: 600px;
	width: auto;
	margin-top: -20px;
	background-image: image-url("trail.jpg");
	background-size: 100% 100%;
}

.jum_image{
	background-repeat: no-repeat;
	height: 700px;
}

.banner {
	background-position: top left, center center;
	background-repeat: repeat, no-repeat;
	background-size: auto, cover;
	color: #fff;
	padding: 12em 0;
	position: relative;
	text-align: center
}

.inner {
	display: inline-block;
	background: rgba(29,39,38,0.7);
	position: relative;
	z-index: 2;
	padding: 25px 25px 25px 25px;
	text-align: center;
}

h3 {
	font-size: 40px;
	color: white;
}

.inner_font {
	font-size: 35px;
	color: white;
}

/* one */ 

.wrapper.style1 {
	background: #006633;	
}

.wrapper {
	padding: 12em 0 12em 0;
	display: block;
	margin: 0;
	border: 0;
	font-size: 100%;
	vertical-align: baseline;
}

.container_one {
	margin-left: auto;
	margin-right: auto;
	width: 960px;

}

.section_p {
	color: black;
	line-height: 25px;
	font-size: 20px
}

.inner1 {
	margin-top: -4em;
	padding-right: 4em;
	display: inline-block;
	vertical-align: middle;
	width: 50%;
}

header.major {
	position: relative;
}

.h2_one {
	font-size: 1.5em;
	line-height: 1.75em;
	font-weight: 500;
	letter-spacing: 0.008em;
	text-transform: uppercase;
}

#one .device {
	border-width: 20px;
	border: solid 20px #fff;
	border-radius: 20px;
	display: inline-block;
	margin-bottom: 0;
	position: relative; 
	vertical-align: middle;
	inner-width: 400px;
	float: right;
}

#one .device .image.fit {
	margin: 0;
	outer-width: 400px;
}

/* Section Two */

.one {
	border: 5px solid black;
	border-radius: 50px;
	margin: 100px auto 100px auto;
	background: rgba(255,255,255,0.9);
	text-align: center;
	padding-top: 25px
}

.circle {
	border: 3px solid black;
	border-radius: 100px;
	padding: 10px;
}

.circle2 {
	border: 3px solid black;
	border-radius: 100px;
	padding: 10px;
	margin-bottom: 25px;
}

.sub_two {
	margin-left: -25px;
	text-align: left
}

/* Image Work */

.image {
	width: 300px;
	height: 300px;
}

.photo {
	margin-top: 10%;
